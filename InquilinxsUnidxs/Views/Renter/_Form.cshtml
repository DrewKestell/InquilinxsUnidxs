@using InquilinxsUnidxs.Presenters

@model RenterPresenter

@Html.Partial("_KOErrors")

<div class="form-group">
    <label>First Name</label>
    <input class="form-control" type="text" data-bind="value: firstName "/>
</div>

<div class="form-group">
    <label>Last Name</label>
    <input class="form-control" type="text" data-bind="value: lastName" />
</div>

<div class="form-group">
    <label>Address 1</label>
    <input class="form-control" type="text" data-bind="value: address1" />
</div>

<div class="form-group">
    <label>Address 2</label>
    <input class="form-control" type="text" data-bind="value: address2" />
</div>

<div class="form-group">
    <label>City</label>
    <input class="form-control" type="text" data-bind="value: city" />
</div>

<div class="form-group">
    <label>State</label>
    <select class="form-control" data-bind="options: allStates, optionsValue: 'Item1', optionsText: 'Item2', value: stateId, optionsCaption: 'Choose a State...'"></select>
</div>

<div class="form-group">
    <label>ZIP</label>
    <input class="form-control" type="text" data-bind="value: zip" />
</div>

<a href="#" class="btn btn-primary" data-bind="click: save">Save</a>